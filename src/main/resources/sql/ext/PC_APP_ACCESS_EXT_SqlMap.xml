<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="PC_APP_ACCESS">



	<select id="selectListByCode" parameterClass="java.util.Map" resultMap="queryResult">
		select 
			<include refid="PC_APP_ACCESS.sql_query_columns" />
		from PC_APP_ACCESS 
		where ACCESS_CODE = #code:VARCHAR#
			<include refid="PC_APP_ACCESS.sql_query_where"/>
		order by 
			<isNotEmpty property="orders" >
				$orders$
			</isNotEmpty>
			<isEmpty property="orders" >
				ID
			</isEmpty>
	</select>



</sqlMap>
