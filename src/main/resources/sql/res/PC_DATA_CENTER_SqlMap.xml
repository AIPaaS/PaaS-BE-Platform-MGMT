	<select id="selectList" parameterClass="java.util.Map" resultMap="queryResult">
		select 
			<include refid="PC_DATA_CENTER.sql_query_columns" />
		from PC_DATA_CENTER 
			<dynamic prepend="where" >
				<include refid="PC_DATA_CENTER.sql_query_where"/>
			</dynamic>
		order by 
			<isNotEmpty property="orders" >
				$orders$
			</isNotEmpty>
			<isEmpty property="orders" >
				ID
			</isEmpty>
	</select>
	<select id="selectCount" parameterClass="java.util.Map" resultClass="java.lang.Long">
		select count(1) from PC_DATA_CENTER 
			<dynamic prepend="where" >
				<include refid="PC_DATA_CENTER.sql_query_where"/>
			</dynamic>
	</select>
	<select id="selectById" parameterClass="java.util.Map" resultMap="queryResult">
		select 
			<include refid="PC_DATA_CENTER.sql_query_columns" />
		from PC_DATA_CENTER where ID=#id:BIGINT# and DATA_STATUS=1  
	</select>
	

	

	<insert id="insert" parameterClass="java.util.Map">
		insert into PC_DATA_CENTER(
			ID, CODE, NAME, REMARK, CUSTOM_1, 
			CUSTOM_2, CUSTOM_3, CUSTOM_4, CUSTOM_5, CUSTOM_6, 
			STATUS, DATA_STATUS, CREATE_TIME, MODIFY_TIME)
		values (
			#record.id:BIGINT#, #record.code:VARCHAR#, #record.name:VARCHAR#, #record.remark:VARCHAR#, #record.custom1:BIGINT#, 
			#record.custom2:BIGINT#, #record.custom3:BIGINT#, #record.custom4:VARCHAR#, #record.custom5:VARCHAR#, #record.custom6:VARCHAR#, 
			#record.status:INTEGER#, #record.dataStatus:INTEGER#, #record.createTime:BIGINT#, #record.modifyTime:BIGINT#)
	</insert>
	

	

	<update id="updateById" parameterClass="java.util.Map">
		update PC_DATA_CENTER
			<dynamic prepend="set" > 
				<include refid="PC_DATA_CENTER.sql_update_columns"/> 
			</dynamic>
		where ID = #id:BIGINT#
	</update>
	<update id="updateByCdt" parameterClass="java.util.Map">
		update PC_DATA_CENTER
			<dynamic prepend="set" > 
				<include refid="PC_DATA_CENTER.sql_update_columns"/> 
