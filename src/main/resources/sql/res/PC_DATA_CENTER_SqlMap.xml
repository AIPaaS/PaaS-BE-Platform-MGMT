	<resultMap class="com.aic.paas.provider.ps.bean.PcDataCenter" id="queryResult">
		<result column="ID" jdbcType="BIGINT" property="id"/>	<!-- ID -->
		<result column="CODE" jdbcType="VARCHAR" property="code"/>	<!-- 数据中心代码 -->
		<result column="NAME" jdbcType="VARCHAR" property="name"/>	<!-- 数据中心名称 -->
